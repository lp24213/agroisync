# Disable YAML validation for GitHub Actions workflows
# This file tells the YAML linter to ignore validation errors

disable: true
validate: false
schema: false
rules:
  line-length: disable
  trailing-spaces: disable
  truthy: disable
  key-duplicates: disable
  empty-lines: disable
  comments-indentation: disable
  document-start: disable
  document-end: disable
  empty-document: disable
  trailing-dots: disable
