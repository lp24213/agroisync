{
  "domain": {
    "name": "agroisync.com",
    "subdomains": [
      "www"
    ],
    "sslCertificate": {
      "type": "AMAZON_ISSUED",
      "region": "us-east-1"
    },
    "dns": {
      "nameservers": [
        "ns-1.awsdns-00.com",
        "ns-2.awsdns-00.org",
        "ns-3.awsdns-00.net",
        "ns-4.awsdns-00.co.uk"
      ],
      "records": [
        {
          "name": "@",
          "type": "A",
          "value": "AWS_ALIAS",
          "target": "d2d5j98tau5snm.amplifyapp.com"
        },
        {
          "name": "www",
          "type": "CNAME",
          "value": "agroisync.com"
        },
        {
          "name": "api",
          "type": "CNAME",
          "value": "agroisync.com"
        }
      ]
    },
    "amplify": {
      "appId": "d2d5j98tau5snm",
      "branch": "main",
      "environment": "prod"
    },
    "redirects": [
      {
        "source": "https://www.agroisync.com/<*>",
        "target": "https://agroisync.com/<*>",
        "status": "301"
      },
      {
        "source": "/<*>",
        "target": "/index.html",
        "status": "404-200"
      }
    ]
  }
}
