{
  "version": "1",
  "storage": {
    "resourceName": "agroisyncstorage",
    "serviceName": "S3",
    "bucketName": "agroisync-storage",
    "bucketRegion": "us-east-2",
    "bucketAccess": "auth",
    "bucketAccessPolicies": "private",
    "bucketEncryption": "SSE-S3",
    "bucketVersioning": "enabled",
    "bucketPublicAccess": "false",
    "bucketTags": [
      {
        "Key": "Project",
        "Value": "AGROISYNC"
      },
      {
        "Key": "Environment",
        "Value": "production"
      }
    ]
  }
}
