{
  "name": "agroisync",
  "version": "1.0.0",
  "description": "AgroSync Frontend - A plataforma de agronegócio mais futurista do mundo",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "build:production": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "test": "cd frontend && npm test",
    "deploy": "npm run build",
    "next:dev": "npm --prefix frontend-next run dev",
    "next:build": "npm --prefix frontend-next run build && npm --prefix frontend-next run postbuild",
    "next:start": "npm --prefix frontend-next run start"
  },
  "keywords": [
    "agronegócio",
    "marketplace",
    "fretes",
    "cripto",
    "nft",
    "defi",
    "futurista",
    "enterprise",
    "frontend"
  ],
  "author": "AgroSync Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.1",
    "imapflow": "^1.2.4",
    "jsdom": "^27.4.0",
    "nodemailer": "^7.0.12",
    "resend": "^6.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "axios": "^1.12.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "lighthouse": "^12.8.2",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "puppeteer": "^21.11.0"
  }
}
