  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "cross-env NODE_ENV=production REACT_APP_API_URL=https://agroisync.com/api REACT_APP_TURNSTILE_SITE_KEY=0x4AAAAAAB3pdjs4jRKvAtaA GENERATE_SOURCEMAP=false react-scripts build && node scripts/generate-sitemap.js",
    "build:production": "cross-env NODE_ENV=production REACT_APP_API_URL=https://agroisync.com/api GENERATE_SOURCEMAP=false react-scripts build && node scripts/generate-sitemap.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "postbuild": "node scripts/generate-sitemap.js",
    "test:a11y": "axe http://localhost:3000 http://localhost:3000/sobre http://localhost:3000/contato http://localhost:3000/produtos http://localhost:3000/frete --exit 0",
    "ci:build": "npm run build",
    "ci:start": "serve -s build -p 3000",
    "ci:lhci": "wait-on http://localhost:3000 && lhci collect --config=./lighthouserc.json && lhci assert --config=./lighthouserc.json",
    "cf:build": "npm run build",
    "cf:deploy": "wrangler pages deploy build --project-name=agroisync",
    "generate:csp": "node scripts/csp-hashes.js",
    "generate:app-assets": "node scripts/generate-app-assets.js",
    "cap:add:android": "npm run build && npx cap add android",
    "cap:add:ios": "npm run build && npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:verify": "node scripts/verify-capacitor-build.js",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:build:android": "npm run build && npm run cap:verify && npx cap sync && npx cap open android",
    "cap:build:ios": "npm run build && npm run cap:verify && npx cap sync && npx cap open ios",
    "screenshots:mobile": "npx playwright test scripts/screenshots.spec.js --project=chromium-mobile",
    "screenshots:desktop": "npx playwright test scripts/screenshots.spec.js --project=chromium-desktop",
    "screenshots:all": "npm run screenshots:mobile && npm run screenshots:desktop",
    "screenshots:prod": "node scripts/generate-screenshots-production.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
