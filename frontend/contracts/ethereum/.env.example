# Chave privada da carteira para deploy (sem o 0x)
PRIVATE_KEY=your_private_key_here

# API Keys para verificação de contratos
ETHERSCAN_API_KEY=your_etherscan_api_key_here
BSCSCAN_API_KEY=your_bscscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# API Key para Infura (para conexão com redes Ethereum)
INFURA_API_KEY=your_infura_api_key_here

# API Key para CoinMarketCap (para relatórios de gas)
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Configuração para relatórios de gas
REPORT_GAS=true

# Endereço do administrador que receberá as comissões
ADMIN_ADDRESS=your_admin_address_here

# Taxa de comissão em pontos base (ex: 500 = 5%)
COMMISSION_RATE=500