{
  "cors": {
    "description": "AGROTM Global CORS Configuration - No Region Restrictions",
    "origin": "*",
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
    "allowedHeaders": [
      "Content-Type",
      "Authorization",
      "X-Requested-With",
      "X-API-Key",
      "X-Client-Version",
      "Origin",
      "Accept"
    ],
    "credentials": true,
    "maxAge": 86400,
    "global": true,
    "regions": {
      "supported": ["BR", "US", "EU", "AS", "AF", "AU", "SA"],
      "restrictions": "none"
    }
  }
}
