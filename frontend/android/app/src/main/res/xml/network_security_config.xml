<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">agroisync.com</domain>
        <domain includeSubdomains="true">*.agroisync.com</domain>
        <domain includeSubdomains="true">*.agroisync.pages.dev</domain>
        <domain includeSubdomains="true">*.cloudflare.com</domain>
        <domain includeSubdomains="true">*.cloudflaressl.com</domain>
        <domain includeSubdomains="true">agroisync.pages.dev</domain>
        <domain includeSubdomains="true">*.workers.dev</domain>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </domain-config>
    <debug-overrides>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </debug-overrides>
</network-security-config>