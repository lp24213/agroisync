            "legendFormat": "Total Users"
          },
          {
            "expr": "agrotm_total_transactions",
            "legendFormat": "Total Transactions"
          },
          {
            "expr": "agrotm_total_staked_amount",
            "legendFormat": "Total Staked Amount"
          },
          {
            "expr": "agrotm_total_nft_minted",
            "legendFormat": "Total NFTs Minted"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
