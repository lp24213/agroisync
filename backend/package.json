{
  "name": "@agroisync/backend",
  "version": "2.3.1",
  "description": "AGROISYNC Backend - Node.js API with AWS Lambda support",
  "private": true,
  "main": "dist/handler.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "build:clean": "npm run clean && npm run build",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:lambda": "node dist/handler.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "type-check:final": "tsc --noEmit --skipLibCheck",
    "clean": "rm -rf dist node_modules/.cache logs uploads temp",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t agroisync-backend .",
    "docker:run": "docker run -p 3001:3001 agroisync-backend",
    "docker:push": "docker tag agroisync-backend:latest your-registry/agroisync-backend:latest && docker push your-registry/agroisync-backend:latest",
    "deploy:sam": "sam build && sam deploy --guided",
    "deploy:sam:prod": "sam build && sam deploy --config-env production",
    "deploy:lambda": "npm run build && sam deploy --config-env production",
    "deploy:docker": "npm run docker:build && npm run docker:push",
    "monitor": "pm2 monit",
    "logs": "pm2 logs",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "health:detailed": "curl -f http://localhost:3001/health/detailed || exit 1"
  },
