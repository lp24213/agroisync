{
  "name": "backend",
  "version": "2.3.1",
  "description": "AGROISYNC Backend API",
  "private": true,
  "main": "dist/handler.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json || echo \"no ts\"",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run || echo \"no tests\"",
    "bundle": "esbuild src/handler.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/handler.mjs"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "agroisync",
    "blockchain",
    "defi",
    "agriculture",
    "nodejs",
    "typescript"
  ],
  "author": "AGROISYNC Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lp24213/agroisync.git"
  },
  "bugs": {
    "url": "https://github.com/lp24213/agroisync/issues"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.152",
    "aws-serverless-express": "^3.4.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "esbuild": "^0.25.9"
  }
}
