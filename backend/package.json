{
  "name": "@agroisync/backend",
  "version": "2.3.1",
  "description": "AGROISYNC Backend - Node.js API with AWS Lambda support",
  "private": true,
  "main": "dist/handler.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "build:clean": "npm run clean && npm run build",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:lambda": "node dist/handler.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache logs uploads temp",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security": "npm audit",
    "deploy:sam": "sam build && sam deploy --guided",
    "health": "curl -f http://localhost:3001/health || exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.873.0",
    "@aws-sdk/client-dynamodb": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/client-secrets-manager": "^3.873.0",
    "@aws-sdk/lib-dynamodb": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.873.0",
    "@types/aws-lambda": "^8.10.152",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "redis": "^5.8.2",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.4.4",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lp24213/agroisync.git"
  },
  "keywords": [
    "agroisync",
    "blockchain",
    "agriculture",
    "api",
    "lambda",
    "aws",
    "typescript",
    "nodejs"
  ],
  "author": "AGROISYNC Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lp24213/agroisync/issues"
  },
  "homepage": "https://github.com/lp24213/agroisync#readme"
}
