{
  "env": {
    "node": true,
    "es2021": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "no-undef": "warn",
    "prettier/prettier": "off"
  }
}
