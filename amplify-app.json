{
  "appId": "d3nvjszcpksd6p",
  "appName": "agroisync",
  "frontend": {
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "outputDirectory": "out",
    "devCommand": "npm run dev",
    "startCommand": "npm run start"
  },
  "rewrites": [
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
